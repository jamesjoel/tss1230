<%- include("../includes/header") %>


<div class="container" style="min-height: 700px;">
    <h1>List of All Student</h1>
    <div class="row">
        <div class="col-md-12">
            <table class="table table-info table-bordered">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Contact</th>
                </tr>
                <% result.forEach((x, n)=>{ %>
                    <tr>
                        <td><%= n+1 %></td>
                        <td><%= x.name %></td>
                        <td><%= x.email %></td>
                        <td><%= x.contact %></td>
                    </tr>
                <% })  %>
            </table>
        </div>
    </div>
    
</div>
